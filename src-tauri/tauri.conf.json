{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "tauri-executor",
  "version": "0.1.0",
  "identifier": "com.tauri.executor",
  "build": {
    "beforeDevCommand": "node serve.js",
    "beforeBuildCommand": "",
    "frontendDist": "../src",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Tauri Executor",
        "width": 800,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "binaries/*"
    ],
    "externalBin": [
      "binaries/dialog-tool"
    ]
  },
  "plugins": {
    "shell": {
      "all": false,
      "execute": true,
      "sidecar": true,
      "open": true
    }
  }
}
